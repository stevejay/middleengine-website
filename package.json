{
  "name": "middleengine-website",
  "description": "Web site for Middle Engine Software",
  "repository": "github:stevejay/middleengine-website",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "watch": "chokidar \"src/**/*\" --initial true -c \"npm run build\"",
    "build": "node ./build.js",
    "serve": "ws --directory build --static.extensions html --port 8001",
    "upgrade-interactive": "npm-check --update"
  },
  "devDependencies": {
    "@nodelib/fs.walk": "^1.2.4",
    "chokidar-cli": "^2.1.0",
    "dotenv": "^8.2.0",
    "fs-extra": "^9.0.0",
    "handlebars": "^4.7.6",
    "handlebars-intl": "^1.1.2",
    "highlight.js": "^9.18.1",
    "local-web-server": "^4.0.0",
    "lodash": "^4.17.15",
    "markdown-it": "^10.0.0",
    "markdown-it-anchor": "^5.2.7",
    "markdown-it-attribution": "^0.1.1",
    "markdown-it-front-matter": "^0.2.1",
    "markdown-it-meta": "0.0.1",
    "markdown-it-multimd-table": "^4.0.1",
    "node-fetch": "^2.6.0",
    "normalize.css": "^8.0.1",
    "npm-check": "^5.9.2",
    "probe-image-size": "^5.0.0",
    "rev-hash": "^3.0.0",
    "unsplash-js": "^6.0.0"
  },
  "engines": {
    "node": ">= 12.0.0"
  }
}
